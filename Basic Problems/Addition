import java.util.*;
public class Add2no{
    public static void main(String []args){
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter first number");
        int k=sc.nextInt();
        System.out.println("Enter second number");
        int a=sc.nextInt();
        int result=k+a;
        System.out.println("The sum of the two numbers is: " + result);
        }
}
